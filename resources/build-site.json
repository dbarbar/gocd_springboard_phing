{
  "id": "%ID%",
  "fullname": "%ID%",
  "environments": {
    "_default": {
      "release_url": "%RELEASE_URL%",
      "release_tag": "%ID%",
      "release_namespace": "custom/example",
      "docroot": "/var/www/%ID%",
      "site_dir": "/var/www/%ID%/sites/default",
      "db_password": "%DB_PASSWORD%",
      "settings_php_conf": {
        "custom_variable_name": "custom_variable_value",
        "custom_variable_name2": "custom_variable_value2"
      },
      "canonical_domain": "%DOMAIN%",
      "domains": {
        "%DOMAIN%": {
          "redirect_all_to_domain": true
        }
      },
      "drupal_hash_salt": "abcdefghijklmnopqrstuvwxyz0123456789-------",
      "encrypt_key": "1234567891234567891234567891234567891234567891234567891234567890"
    }
  }
}
